# Example .env for Celestial Mechanics Calculations API
# Copy this file to `.env` and edit values as required.

# Application
APP_NAME="Celestial Mechanics Calculations API"
APP_VERSION="0.1.0"
DEBUG=False

# Server
HOST=0.0.0.0
PORT=8000

# CORS origins: recommended as a JSON array. Alternative: a comma-separated string.
# Examples:
#   CORS_ORIGINS='["http://localhost:3000","http://localhost:5173"]'
#   CORS_ORIGINS=http://localhost:3000,http://localhost:5173
CORS_ORIGINS='["http://localhost:3000","http://localhost:5173"]'

# API prefix
API_V1_PREFIX="/api/v1"
